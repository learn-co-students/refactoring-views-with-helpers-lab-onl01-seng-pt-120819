<ul>
  <% @songs.each do |song| %>
    <% if song.artist%>
      <li><%= link_to song.title, song_path(song) %>
          <%= display_artist song %></li>
    <% else %>  
      <%= link_to "Add Artist", edit_song_path(song) %>
    <% end %>
  <% end %>
</ul>

<br>

<%= link_to "Add New Song", new_song_path %>
